<?xml version='1.0' encoding='utf-8'?>
<widget id="sg.com.bbh-labs.sensenet" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SenseNet</name>
    <description>
        SenseNet client app for browsing and sending sensor data.
	</description>
    <author email="jacky.boen@bartleboglehegarty.com" href="https://bbh-labs.com.sg">
        BBH Labs team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <allow-navigation href="http://*" />
    <allow-navigation href="https://*" />
    <allow-navigation href="ws://*" />
    <allow-navigation href="wss://*" />
    <access origin="http://*/*" />
    <access origin="https://*/*" />
    <access origin="ws://*/*" />
    <access origin="wss://*/*" />
    <access origin="content:///*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="ws://*/*" />
    <allow-intent href="wss://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
